      <div class="offset-lg-2">
        <div class="container title" style="background-color: white !important;">
          <h1>GET A CALL BACK </h1>
        </div>
        <form id="form">
          <div class="field">
            <label for="name">Name</label>
            <input type="text" name="name" id="name" required>
          </div>
          <div class="field">
            <label for="email">Email</label>
            <input type="text" name="email" id="email" required>
          </div>
          <div class="field">
            <label for="phone">Phone</label>
            <input type="text" name="phone" id="phone" required>
          </div>
          <div class="field">
            <label for="message">Message</label>
            <input type="text" name="message" id="message">
          </div>

          <input type="submit" id="button" value="Request Call Back">
        </form>

        <div id="success-message" class="alert alert-success" style="display: none;">
          Data Sent Successfully!
        </div>

        <div id="error-message" class="alert alert-danger" style="display: none;">
          Error Sending Data. Please try again later.
        </div>

        <script type="text/javascript" src="https://cdn.jsdelivr.net/npm/@emailjs/browser@3/dist/email.min.js"></script>

        <script type="text/javascript">
          document.addEventListener('DOMContentLoaded', function () {
            emailjs.init('MzB0DoOVUF2se8ar7')

            const form = document.getElementById('form');
            const successMessage = document.getElementById('success-message'); // Selecting success message element
            const errorMessage = document.getElementById('error-message');
            const serviceID = 'service_66kaiwe';
            const templateID = 'template_bqmqcs4';

            form.addEventListener('submit', function (event) {
              event.preventDefault();

              emailjs.sendForm(serviceID, templateID, this)
                .then(() => {
                  // Reset the form first
                  form.reset();
                  // Show success message
                  successMessage.style.display = 'block'; // Displaying success message
                  errorMessage.style.display = 'none'; // Hiding error message if previously displayed
                })
                .catch((error) => {
                  // Show error message
                  errorMessage.style.display = 'block';
                  console.error('EmailJS sendForm error:', error);
                });
            });
          });
        </script>
      </div>
