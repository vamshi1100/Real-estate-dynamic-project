 <main>
    <div class="modal fade" id="Request" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
      <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
          <div class="modal-header border-bottom-0">
            <button type="button" class="btn-close" data-bs-dismiss="modal" data-dismiss="modal"></button>
          </div>
          <div class="modal-body">
            <div class="form-title text-center">
              <h4 class="text-info">Quick Enquiry</h4>
            </div>
            <div class="d-flex flex-column text-center">
              <form>
                <div class="form-group p-2">              
                  <input type="text" class="form-control" id="name"placeholder="Name..." required>
                </div>
                <div class="form-group p-2">                 
                  <input type="email" class="form-control" id="email"placeholder="Email Address..."required>
                </div>
                <div class="form-group p-2">                
                  <input type="tel" class="form-control" id="password" pattern="[0-9]{3}-[0-9]{2}-[0-9]{3}" placeholder="Phone Number..."required>
                </div>
                <div class="form-group p-2">              
                  <textarea class="form-control" id="message" placeholder="Type your message..." rows="3"required></textarea>
                </div>
                <button type="button" class="btn btn-outline-primary btn-lg rounded-pill">Send</button>
              </form>
            </div>
          </div>
        </div>
      </div>
    </div>
